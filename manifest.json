{
  "manifest_version": 2,
  "name": "Fido Fetch!",
  "version": "1.1",
  "description": "Revisit those forgotten videos!",
  "homepage_url": "https://github.com/dhruvchand/FidoFetch",
  "icons": {
    "48": "icons/bookmark-it.png",
    "96": "icons/bookmark-it@2x.png"
  },

  "permissions": [
    "bookmarks",
    "tabs",
    "storage"
  ],

  "browser_action": {
    "default_icon": "icons/fido-fetch.png",
    "default_title": "Fido Fetch!"
  },

"page_action": {
    "default_icon": "icons/bookmark-it.png",
    "default_title": "Give Fido."
  },

  "background": {
    "scripts": ["background.js"]
  },

"applications": {
  "gecko": {
    "id": "dhruvchand@live.com",
    "strict_min_version": "42.0"
  }
}

}
